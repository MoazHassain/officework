    {{block "style" .}}
        <style>
            body
            {
                margin: 0px;
                background-color: black;
                color: white;
            }
            a
            {
                text-decoration: none;
            }
            a.btn
            {
                color: black;
                text-transform: uppercase;
                padding: 18px 28px;
                background-color: white;
                border-radius: 5px;
                transition: 0.3s;
            }
            a.btn:hover
            {
                background-color: rgba(255, 255, 255, 0.8);
            }
            header
            {
                background-color: black;
                height: 80px;
                display: grid;
                grid-template-columns: 3fr 2.5fr 1fr;
                grid-template-areas: 
                ". logo btn";
                position: sticky;
                top: 0;
                z-index: 9999;
            }
            header .part-one
            {
                grid-area: logo;
                margin: auto 0%;
            }
            .header-title-logo img
            {
                height: 50px; 
            
            }
            header .part-two
            {
                grid-area: btn;
                margin: auto 0%;
            }

            header .part-two a
            {
                font-family: "archivo black", sans-serif;
                letter-spacing: 0.05em;
                padding: 13px 24px;
            }
            .one
            {
                height: 40.5em;
                background-color: white;
                color: black;
                display: grid;
                grid-template-columns: 1fr 1fr;
                /* grid-gap: 2em; */
                
            }
            .one .part-one
            {
                padding: 6em;
                padding-right: 3em;
                padding-left: 7.5em;
                padding-bottom: 0px;
            }
            .one .part-one div.text-area
            {
                font-family: "archivo", sans-serif;
                font-size: 14px;
                font-weight: 500;
                line-height: 25px;
                margin-bottom: 45px;
                letter-spacing: 0.5px;
            }
            
            .one .part-one .text-area p
            {
                font-weight: 600;
                
            }
            .one .part-one h1
            {
                font-family: "Archivo black", sans-serif;
                font-size: 85px;
                letter-spacing: -0.09em;
                line-height: 1em;
                margin: 30px 0px;
                
            }
            .one .part-one button
            {
                font-family: "archivo", sans-serif;
                font-weight: 500;
                background-color: black;
                color: white;
                text-transform: uppercase;
            }
            .one .part-one button:hover
            {
                background-color: rgba(0, 0, 0, 0.8);
            }
            .one .part-two 
            {
                background-image: url(./resources/images/bag-n-brand2.png);
                background-repeat: no-repeat;
                background-size: cover;
                background-position: center;
                margin: 3em 7em;
            }
            .two
            {
                display: grid;
                grid-template-columns: 1fr 1fr;
                grid-gap: 4em;
                width: 85%;
                margin: 3em auto;
            }
            .two .part-one img
            {
                width: 80%;
            }
            .two .part-one h1
            {
                font-family: "archivo black", sans-serif;
                font-size: 80px;
                letter-spacing: -.09em;
                margin: 0px;
            }
            
            .two .part-one p
            {
                font-family: "archivo", sans-serif;
                font-size: 14px;
                font-weight: 600;
                justify-items: center;
                margin-left: 4em;
            }
            .two .part-two ul
            {
                font-family: "archivo", sans-serif;
                font-size: 12px;
                font-weight: 500;
                line-height: 22px;
                margin-left: 7em;
            }
            footer .footer-title-logo img
            {
                height: 60px;
                margin: 40px 0px;
                
            }
            footer .contact a
            {
                color: white;
                text-decoration: underline;
            }
            footer .contact p
            {
                font-family: "archivo", sans-serif;
                font-size: 12px;
                margin-bottom: 25px;
            }
            footer .contact p:last-child
            {
                font-weight: 600;
                margin-bottom: 70px;
            }
            .modal-bg
            {
                display: none;
                position: fixed;
                width: 100%;
                height: 95vh;
                top: 0;
                left: 0;
                padding-bottom: 5em;
                background-color: rgba(0, 0, 0, 0.8);
                display: grid;
                
                justify-content: center;
                align-content: center;
                font-family: 'Segoe UI', sans-serif;
                display: none;
                z-index: 9999;
                overflow-y: scroll;
                overflow-x: hidden;
            }
            
            .modal
            {
                background-color: white;
                width: 27%;
                margin: 0 auto;
                margin-top: 6em;
                margin-bottom: 3em;
                border-radius: 5px;
            }
            .modal .part-one
            {
                text-align: center;
            }
            .modal .part-one h1
            {
                color: black;
                font-size: 28px;
            }
            .modal .part-one img
            {
                width: 30%;
                margin-top: 2em;
            }
            .modal .part-two
            {
                background-color: rgba(250, 250, 250, 0.4);
                padding: 8% 3%;
                box-shadow: 0px 0px 5px 1px #f0f0f0 inset;
            }
            .modal input
            {
                width: 80%;
                margin: 0px 8%;
                margin-top: 5px;
                margin-bottom: 15px;
                padding: 1.2em;
                padding-right: 0px;
                border: 1px solid;
                background-color: #fcfcfc;
                border-color: #ebebeb;
                border-radius: 4px;
                box-shadow: 0px 0px 4px 1px #f0f0f0 inset;
                transition: 0.3s;
            }
            .modal input:hover
            {
                background-color: white;
                box-shadow: 0px 0px 4px 1px #f0f0f0;
            }
            .modal input:focus
            {
                outline: none;
                box-shadow: 0px 0px 4px 1px #f0f0f0;
            }
            .modal input[type="button"]
            {
                font-size: 16px;
                font-weight: 600;
                box-shadow: none;
                padding-right: 1.2em;
                color: white;
                background-color: red;
                width: 84%;
            }
            .modal .part-two p
            {
                font-size: 10px;
                font-weight: 600;
                text-align: center;
                color: #7a7a7a;
            }
            .modal .part-two a
            {
                font-size: 10px;
                color: red;
                text-decoration: underline dotted;
            }
            .modal label
            {
                font-size: 14px;
                font-weight: 700; 
                color: black;
                width: 90%;
                margin: 0px 8%;
                margin-top: 20px;
                padding: 0px 2px ;
                
            }
            
            .modal-bg a 
            {
                text-transform: none;
                font-weight: 700;
                font-size: 14px;
                text-align: center;
                width: 15%;
                margin: 0px auto;
                color: #7a7a7a;
            }
            .modal-bg a span
            {
                color: red;
            }
            .login-modal-bg
            {
                display: none;
                position: fixed;
                width: 100%;
                height: 95vh;
                top: 0;
                left: 0;
                padding-bottom: 5em;
                background-color: rgba(0, 0, 0, 0.8);
                display: grid;
                
                justify-content: center;
                align-content: center;
                font-family: 'Segoe UI', sans-serif;
                display: none;
                z-index: 9999;
                overflow-y: scroll;
                overflow-x: hidden;
            }
            
            .login-modal
            {
                
                background-color: white;
                
                width: 27%;
                margin: 0 auto;
                margin-top: 6em;
                margin-bottom: 3em;
                border-radius: 5px;
            }
            .login-modal .part-one
            {
                text-align: center;
            }
            .login-modal .part-one h1
            {
                color: black;
                font-size: 28px;
            }
            .login-modal .part-one img
            {
                width: 30%;
                margin-top: 2em;
            }
            .login-modal .part-two
            {
                background-color: rgba(250, 250, 250, 0.4);
                padding: 8% 3%;
                box-shadow: 0px 0px 5px 1px #f0f0f0 inset;
            }
            .login-modal input
            {
                width: 80%;
                margin: 0px 8%;
                margin-top: 5px;
                margin-bottom: 15px;
                padding: 1.2em;
                padding-right: 0px;
                border: 1px solid;
                background-color: #fcfcfc;
                border-color: #ebebeb;
                border-radius: 4px;
                box-shadow: 0px 0px 4px 1px #f0f0f0 inset;
                transition: 0.3s;
            }
            .login-modal input:hover
            {
                background-color: white;
                box-shadow: 0px 0px 4px 1px #f0f0f0;
            }
            .login-modal input:focus
            {
                outline: none;
                box-shadow: 0px 0px 4px 1px #f0f0f0;
            }
            .login-modal input[type="button"]
            {
                font-size: 16px;
                font-weight: 600;
                box-shadow: none;
                padding-right: 1.2em;
                color: white;
                background-color: red;
                width: 84%;
            }
            .login-modal .part-two p
            {
                font-size: 12px;
                font-weight: 700;
                color: red;
                text-align: right;
                margin: 10px 8%;
            }
            .login-modal .part-two a
            {
                
                font-size: 12px;
                color: red;
                
            }
            .login-modal label
            {
                font-size: 14px;
                font-weight: 700; 
                color: black;
                width: 90%;
                margin: 0px 8%;
                margin-top: 20px;
                padding: 0px 2px ;
                
            }
            
            .login-modal-bg a 
            {
                text-transform: none;
                font-weight: 700;
                font-size: 14px;
                text-align: center;
                width: 15%;
                margin: 0px auto;
                color: #7a7a7a;
            }
            .login-modal-bg a span
            {
                color: red;
            }
        </style>
        <!-- reponsive -->
        <style>
            /* tab */
            @media only screen and (max-width: 1025px) and (min-width: 760px)
            {
                .one
                {
                    height: unset;
                    grid-gap: 3em;
                    padding: clamp(34px, 4.5vw, 48px);
                }
                .one .part-one
                {
                    padding: 0px;
                }

                .one .part-one h1 
                {
                    font-size: clamp(62px, 8vw, 78px);
                    margin-top: 0;
                }
                .one .part-one .text-area
                {
                    margin-bottom: 0px !important;
                }
                .one .part-one button 
                {
                    margin: clamp(20px, 3vw, 30px) 0;
                }
                .one .part-two
                {
                    margin: 0;
                }
                .two
                {
                    grid-gap: 1em;
                }
                .two .part-one
                {
                    text-align: center;
                }

                .two .part-one p
                {
                    margin: 0;
                }
                .two .part-two li
                {
                    text-align: justify;
                }

            }
            @media only screen and (max-width: 770px) and (min-width: 760px)
            {
                .one .part-two
                {
                    height: 350px;
                }
            }

            /* mobile */
            @media only screen and (max-width: 750px)
            {
                .one
                {
                    height: unset;
                    grid-gap: 2em;
                    padding: clamp(34px, 4.5vw, 48px) clamp(15px, 6vw, 30px);
                    grid-template-columns: 1fr;
                    grid-template-rows: auto auto;
                    grid-template-areas:  
                    "img"
                    "detail";
                    /* justify-content: center; */
                    /* justify-items: center; */
                }
                .one .part-one
                {
                    grid-area: detail;
                    padding: 0px;
                }

                .one .part-one h1 
                {
                    font-size: clamp(40px, 9vw, 46px);
                    margin-top: 0;
                }
                .one .part-one .text-area
                {
                    margin-bottom: 0px !important;
                }
                .one .part-one button 
                {
                    margin: clamp(20px, 3vw, 30px) 0;
                }
                .one .part-two
                {
                    grid-area: img;
                    margin: 0;
                    /* width: clamp(246px, 20vw, 330px); */
                    height: clamp(270px, 90vw, 380px) ;
                    
                }
                .one .part-one button 
                {
                    font-size: 16px;
                    width: 100%;
                    padding: 1em;
                }

                .two
                {
                    margin: 0;
                    margin: clamp(34px, 4.5vw, 48px) clamp(15px, 6vw, 30px);
                    grid-template-columns: auto;
                    grid-gap: 2em;
                    justify-self: center;
                }
                .two .part-one
                {
                    text-align: center;
                }
                .two .part-one h1 
                {
                    font-size: clamp(42px, 12vw, 52px);
                    margin: 10px 0;
                }
                .two .part-one p
                {
                    margin: 0;
                    
                }
                .two .part-two li
                {
                    
                    margin-left: -40px;
                }
                
            }
        </style>

    {{end}}

